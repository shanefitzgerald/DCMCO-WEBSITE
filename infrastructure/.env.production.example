# ============================================
# Production Environment Configuration
# ============================================
#
# This is a pre-configured example for production environment.
# Copy this file to .env:
#   cp .env.production.example .env
#
# Then update the values with your actual production configuration.
#
# ⚠️  WARNING: Double-check all values before deploying to production!

# GCP Configuration
GCP_PROJECT_ID=your-production-project-id
GCP_REGION=australia-southeast1
# GCP_ZONE=australia-southeast1-a

# Environment
ENVIRONMENT=production

# GCS Bucket Configuration
GCS_BUCKET_NAME=your-project-website-prod
GCS_BUCKET_LOCATION=AUSTRALIA-SOUTHEAST1

# Domain Configuration
# Uncomment and set your production domain
DOMAIN_NAME=example.com

# Advanced Configuration
# Consider enabling versioning for production
ENABLE_BUCKET_VERSIONING=true
UNIFORM_BUCKET_LEVEL_ACCESS=true
